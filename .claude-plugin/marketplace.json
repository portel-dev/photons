{
  "name": "photons-marketplace",
  "owner": {
    "name": "Portel",
    "email": "contact@portel.dev"
  },
  "metadata": {
    "description": "Official photons MCP servers",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "photons",
      "description": "Complete collection of 17 Photon MCP servers",
      "source": "./",
      "strict": false,
      "hooks": [
        "./.claude-plugin/hooks.json"
      ],
      "mcpServers": {
        "photon-aws-s3": {
          "command": "photon",
          "args": [
            "mcp",
            "aws-s3"
          ]
        },
        "photon-docker": {
          "command": "photon",
          "args": [
            "mcp",
            "docker"
          ]
        },
        "photon-email": {
          "command": "photon",
          "args": [
            "mcp",
            "email"
          ]
        },
        "photon-fetch": {
          "command": "photon",
          "args": [
            "mcp",
            "fetch"
          ]
        },
        "photon-filesystem": {
          "command": "photon",
          "args": [
            "mcp",
            "filesystem"
          ]
        },
        "photon-git": {
          "command": "photon",
          "args": [
            "mcp",
            "git"
          ]
        },
        "photon-github-issues": {
          "command": "photon",
          "args": [
            "mcp",
            "github-issues"
          ]
        },
        "photon-google-calendar": {
          "command": "photon",
          "args": [
            "mcp",
            "google-calendar"
          ]
        },
        "photon-jira": {
          "command": "photon",
          "args": [
            "mcp",
            "jira"
          ]
        },
        "photon-math": {
          "command": "photon",
          "args": [
            "mcp",
            "math"
          ]
        },
        "photon-memory": {
          "command": "photon",
          "args": [
            "mcp",
            "memory"
          ]
        },
        "photon-mongodb": {
          "command": "photon",
          "args": [
            "mcp",
            "mongodb"
          ]
        },
        "photon-postgres": {
          "command": "photon",
          "args": [
            "mcp",
            "postgres"
          ]
        },
        "photon-redis": {
          "command": "photon",
          "args": [
            "mcp",
            "redis"
          ]
        },
        "photon-slack": {
          "command": "photon",
          "args": [
            "mcp",
            "slack"
          ]
        },
        "photon-sqlite": {
          "command": "photon",
          "args": [
            "mcp",
            "sqlite"
          ]
        },
        "photon-time": {
          "command": "photon",
          "args": [
            "mcp",
            "time"
          ]
        }
      }
    }
  ]
}